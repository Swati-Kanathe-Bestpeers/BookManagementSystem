<div class="container">
  <h2>Add new data</h2>
  <%= form_with model: @author do |f| %>
    <%= f.label :first_name, "First Name" %>
    <%= f.text_field :first_name%>
    <br> <br>
    <%= f.label :last_name, "Last Name" %>
    <%= f.text_field :last_name %>
    <br> <br>
    <%= f.label :title, "Title" %>
    <%= f.text_field :title %>
    <br> <br>
    <%= f.submit "Create" %>
  <% end %>
</div>